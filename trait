while true do
    local getNil = function(name, class)
        for _, v in next, getnilinstances() do
            if v.ClassName == class and v.Name == name then
                return v
            end
        end
    end
    
    local playerUsername = game.Players.LocalPlayer.Name
    
    local player = game.Players:FindFirstChild(playerUsername)
    if player then
        local playerGui = player:FindFirstChild("PlayerGui")
        if playerGui then
            local traitCrystal = playerGui.PAGES:FindFirstChild("ItemPage")
                and playerGui.PAGES.ItemPage:FindFirstChild("ScrollingFrame")
                and playerGui.PAGES.ItemPage.ScrollingFrame:FindFirstChild("Trait Crystal")
            if traitCrystal then
                local button = traitCrystal:FindFirstChild("Button")
                if button then
                    local countLabel = button:FindFirstChild("CountLabel")
                    if countLabel and countLabel:IsA("TextLabel") then
                        countLabel.Text = "1000x"
                        print("")
                    else
                        print("")
                    end
                else
                    print("")
                end
            else
                print("")
            end
            
            local inventoryCountLabel = playerGui.PAGES:FindFirstChild("TraitPage")
                and playerGui.PAGES.TraitPage:FindFirstChild("InventoryTokens")
                and playerGui.PAGES.TraitPage.InventoryTokens:FindFirstChild("InventoryCountLabel")
            
            if inventoryCountLabel and inventoryCountLabel:IsA("TextLabel") then
                inventoryCountLabel.Text = "1000x"
                print("")
            else
                print("")
            end
        else
            print("")
        end
    else
        print("")
    end
    wait(1)
end
